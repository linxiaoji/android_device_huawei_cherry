# Android fstab file.
#<src>                                                  <mnt_point>         <type>      <mnt_flags and options>             <fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

/dev/block/platform/hi_mci.0/by-name/boot	/boot		emmc	defaults					recoveryonly
/dev/block/platform/hi_mci.0/by-name/recovery	/recovery	emmc	defaults					recoveryonly
/dev/block/platform/hi_mci.0/by-name/misc	/misc		emmc	defaults					recoveryonly
/dev/block/platform/hi_mci.0/by-name/sensorhub	/sensorhub	emmc	defaults					recoveryonly

/dev/block/platform/hi_mci.0/by-name/system	/system		ext4	ro						wait
/dev/block/platform/hi_mci.0/by-name/cache	/cache		ext4 	noatime,nosuid,nodev,discard			wait,check
/dev/block/platform/hi_mci.0/by-name/userdata	/data		ext4	noatime,nosuid,nodev,discard,noauto_da_alloc	wait,check,encryptable=footer

/dev/block/platform/hi_mci.0/by-name/modemnvm1	/mnvm1:0	ext4	defaults					wait
/dev/block/platform/hi_mci.0/by-name/modemnvm2	/mnvm2:0	ext4	defaults					wait
/dev/block/platform/hi_mci.0/by-name/modemnvm3	/mnvm3:0	ext4	defaults					wait
/dev/block/platform/hi_mci.0/by-name/modemlog	/modem_log	ext4	defaults					wait

/devices/hi_mci.1/mmc_host/mmc1*		auto		auto	defaults					voldmanaged=sdcard1:auto,noemulatedsd
/devices/hi6xxx-usb-otg/usb*			auto		auto	defaults					voldmanaged=usbdisk:auto
